{
  "name": "Custom Chrome Extension",
  "description": "My custom chrome extension to cut down on official chrome extensions.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
  ],
  "background": {
    "service_worker": "js/background.js",
    "type": "module"
  },
  "commands": {
    "my-global-function": {
      "suggested_key": {
        "default": "Ctrl+D",
        "mac": "MacCtrl+D"
      },
      "description": "Capture the current tab"
    }
  },
  "action": {
    "default_title": "Org Capture",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  }
}
