{
  "name": "Custom Chrome Extension",
  "description": "My custom chrome extension to cut down on official chrome extensions.",
  "version": "1.1",
  "manifest_version": 3,
  "permissions": ["tabs"],
  "background": {
    "service_worker": "js/background.js",
    "type": "module"
  },
  "commands": {
    "close-other-tabs": {
      "suggested_key": {
        "default": "Ctrl+Shift+T"
      },
      "description": "Close other tabs"
    },
    "pin-tab": {
      "suggested_key": {
        "default": "Alt+P"
      },
      "description": "Pin current tab"
    }
  },
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  }
}
